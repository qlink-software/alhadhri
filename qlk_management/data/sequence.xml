<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_proposal" model="ir.sequence">
        <field name="name">Proposal</field>
        <field name="code">proposal.sequence</field>
        <field name="prefix">Prop</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"></field>
    </record>
    <record id="seq_agreement" model="ir.sequence">
        <field name="name">Agreement</field>
        <field name="code">agreement.sequence</field>
        <field name="prefix">Agre</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"></field>
    </record>

    <record id="seq_bd_engagement_letter_retainer" model="ir.sequence">
        <field name="name">Engagement Letter Retainer</field>
        <field name="code">bd.engagement.letter.retainer</field>
        <field name="prefix">AH/EL/R/</field>
        <field name="suffix">/%(year)s</field>
        <field name="padding">3</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"></field>
    </record>

    <record id="seq_bd_engagement_letter_lump_sum" model="ir.sequence">
        <field name="name">Engagement Letter Lump Sum</field>
        <field name="code">bd.engagement.letter.lump_sum</field>
        <field name="prefix">AH/EL/L/</field>
        <field name="suffix">/%(year)s</field>
        <field name="padding">3</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"></field>
    </record>

    <record id="seq_bd_client_code" model="ir.sequence">
        <field name="name">BD Client Code</field>
        <field name="code">bd.client.code</field>
        <field name="prefix">C-%(year)s-</field>
        <field name="padding">4</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"></field>
    </record>


</odoo>
