<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_engagement_proposal" model="ir.sequence">
            <field name="name">Engagement Proposal</field>
            <field name="code">qlk.engagement.proposal</field>
            <field name="prefix">AH/EP/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_business_proposal" model="ir.sequence">
            <field name="name">Business Proposal</field>
            <field name="code">qlk.business.proposal</field>
            <field name="prefix">AH/BP/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_proposal_initiation" model="ir.sequence">
            <field name="name">Proposal Initiation</field>
            <field name="code">qlk.proposal.initiation</field>
            <field name="prefix">AH/PI/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
